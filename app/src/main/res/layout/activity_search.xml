<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@drawable/background2"    tools:context=".search">    <Button        android:id="@+id/Delete_fr"        android:layout_width="328dp"        android:layout_height="56dp"        android:background="@drawable/raduis_btn"        android:text="Delete"        android:textColor="#13E861"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.493"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.674" />    <com.google.android.material.textfield.TextInputEditText        android:id="@+id/search_inp2"        android:layout_width="362dp"        android:layout_height="79dp"        android:layout_marginStart="4dp"        android:background="@drawable/raduis_input_search"        android:hint="search"        android:padding="10dp"        android:textColor="#00CF91"        android:textColorHint="#ED0000"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.183" />    <com.google.android.material.textfield.TextInputEditText        android:id="@+id/search_inp"        android:layout_width="357dp"        android:layout_height="72dp"        android:layout_marginStart="4dp"        android:autoText="true"        android:background="@drawable/raduis_input_search"        android:hint="search"        android:padding="10dp"        android:textColor="#00CF91"        android:textColorHint="#E30F0F"        android:textColorLink="#FFFFFF"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.024" /></androidx.constraintlayout.widget.ConstraintLayout>